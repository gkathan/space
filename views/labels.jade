include kanban_menu


block content
div(style="margin-left:100px")

			div.panel(style=" position: relative;padding: 20px 40px 40px;display: block;background-color:white;;width:700px;text-align: center;margin: 10px auto;")
				.panel-heading
					h3.panel-title LABELS
				.panel-body
				table.table.table-striped.table-hover.table-condensed
					thead
						tr
							th screen
							th market
							th brand
							th label
					tbody
						each l in labels
							tr
							td(style="text-align:left") 
								img(src="/images/labels/#{l.label}.png"  height="70px") 
							td(style="vertical-align:middle;text-align:left") #{l.market} 
							td(style="vertical-align:middle;text-align:left") #{l.brand} 
							td(style="vertical-align:middle;text-align:left") 
								a(href="https://#{l.label}"  target="_new") #{l.label} 


