					ul.nav.navbar-nav
						li.dropdown(id="fat-menu")
							a.dropdown-toggle(id="org" href="#" data-toggle="dropdown" aria-haspopup="true" role="button" aria-expanded="false") initiatives & portfolio
								span.caret
							ul.dropdown-menu(role="menu" aria-labelledby="portfolio")
								li(role="presentation")
