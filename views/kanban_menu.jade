extends layout

block content
	div.container-fluid
		div.row(style="margin-left:10px;margin-bottom:10px;margin-top:10px")
			if session.CONTEXT
				img(src='/images/#{session.CONTEXT}_logo.png'  height='20px')
				span(style="font-size:10px;margin-left:20px;color:grey")  context: <b> #{session.CONTEXT}</b>
			else
				img(src='/images/#{config.context}_logo.png'  height='20px')

			if session.AUTH
				span(style="font-size:10px;margin-left:20px;color:grey")  logged in as: <b> #{session.AUTH}/#{session.USER} </b>


			.div(class='notifications top-left')

		div.navbar.navbar-default
			.navbar-header
				button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
					span.icon-bar
					span.icon-bar
					span.icon-bar
				if !session.AUTH
					a.navbar-brand(href='/')
						img(src="/images/space_long_white.png" height="30px")
				else
					a.navbar-brand(href='/dashboard')
						img(src="/images/space_long_white.png" height="30px")

			.collapse.navbar-collapse
				ul.nav.navbar-nav

					if !session.AUTH
						li
							a(href='/login') login

						li
							a(role="menuitem" tabindex="-1" href="/firereports") fire reports

					if session.AUTH
						li.active
							a(href='/logout') logout

						ul.nav.navbar-nav
							li.dropdown(id="fat-menu")
								a.dropdown-toggle(id="targets" href="#" data-toggle="dropdown" aria-haspopup="true" role="button" aria-expanded="false") environment
									span.caret

								ul.dropdown-menu(role="menu" aria-labelledby="admin")
									li(role="presentation")
										a(role="menuitem" tabindex="-1" href="/customers") customers
									li(role="presentation")
